# Server Configuration
NODE_ENV=development
PORT=3001
LOG_LEVEL=info

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Optional: Supabase Service Role Key (for admin operations)
# SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Email Configuration
EMAIL_FROM=notifications@devicue.com
RESEND_API_KEY=your_resend_api_key
FRONTEND_URL=http://localhost:3000

# Notification Scheduler
# Cron expression for notification checks (default: every 15 minutes)
NOTIFICATION_CHECK_CRON=*/15 * * * *
# Set to false to disable email notifications
EMAIL_NOTIFICATIONS_ENABLED=true

# Google Gemini API Configuration (for AI-powered reports)
# Get your free API key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key
